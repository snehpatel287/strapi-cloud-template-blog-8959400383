{
    "kind": "collectionType",
    "collectionName": "articles",
    "info": {
        "singularName": "article",
        "pluralName": "articles",
        "displayName": "Article",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "articleCategory": {
            "type": "enumeration",
            "enum": [
                "Behind The Scenes"
            ]
        },
        "articleTitle": {
            "type": "blocks"
        },
        "author": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::author.author"
        },
        "articleImage": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "articlePublishDate": {
            "type": "date"
        }
    }
}
