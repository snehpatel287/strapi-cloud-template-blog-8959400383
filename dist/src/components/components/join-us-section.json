{
    "collectionName": "components_components_join_us_sections",
    "info": {
        "displayName": "Join Us Section",
        "icon": "handHeart",
        "description": ""
    },
    "options": {},
    "attributes": {
        "title": {
            "type": "blocks"
        },
        "description": {
            "type": "blocks"
        },
        "image": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "button": {
            "type": "component",
            "repeatable": false,
            "component": "atoms.button"
        },
        "sectionColor": {
            "type": "customField",
            "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
            "customField": "plugin::color-picker.color"
        },
        "variant": {
            "type": "enumeration",
            "enum": [
                "brideBeautyPlanner",
                "spaSubpage"
            ]
        },
        "alignment": {
            "type": "enumeration",
            "enum": [
                "left",
                "right"
            ]
        }
    }
}
